# expat config file

choice
    bool
    prompt "expat version"
# Don't remove next line
# CT_INSERT_VERSION_BELOW

config EXPAT_V_2_4_8
    bool
    prompt "2.4.8"

config EXPAT_V_2_2_5
    bool
    prompt "2.2.5"

config EXPAT_V_2_1_0
    bool
    prompt "2.1.0"

endchoice

config EXPAT_VERSION
    string
# Don't remove next line
# CT_INSERT_VERSION_STRING_BELOW
    default "2.4.8" if EXPAT_V_2_4_8
    default "2.2.5" if EXPAT_V_2_2_5
    default "2.1.0" if EXPAT_V_2_1_0
